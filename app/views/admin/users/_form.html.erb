<%= error_messages_for(@episode)%>
<table summary="Episode form fields">
  <tr>
    <th><%= f.label(:first_name, "First Name")%></th>
    <td><%= f.text_field(:first_name) %></td>
  </tr>
  <tr>
    <th><%= f.label(:last_name, "Last Name")%></th>
    <td><%= f.text_field(:last_name) %></td>
  </tr>
  <tr>
    <th><%= f.label(:email, "Email")%></th>
    <td><%= f.text_field(:email) %></td>
  </tr>
  <tr>
    <th><%= f.label(:password, "Password")%></th>
    <td><%= f.password_field(:password) %></td>
  </tr>
  <tr>
    <th><%= f.label(:confirm_password, "Confirm Password")%></th>
    <td><%= f.password_field(:password_confirmation) %></td>
  </tr>
  <tr>
    <th><%= f.label(:access_level, "Access Level")%></th>
    <td><%= f.select(:access_level, {"Author"=>1, "Editor"=>2, "Admin"=>3}) %></td>
  </tr>
</table>
